[user]
	name = Kosma Moczek
[alias]
	ci = commit
	staged = diff --cached
	unstage = reset HEAD
	ss = status --short --branch
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%an <%ae>%Creset' --abbrev-commit
	rollback = reset --soft HEAD~1
[push]
	default = current
[color]
	ui = always
[log]
	decorate = full
[branch]
	autosetuprebase = always
